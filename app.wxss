.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.customer {
  position: fixed;
  right: 20rpx;
  bottom: 60rpx;
  width: 80rpx;
  height: 60rpx;
  background-color: rgba(255, 255, 255, 0);
  border-radius: 50%;
}
.customer view {
  overflow: hidden;
  text-align: center;
}
.customer image {
  width: 80%;
  border-radius: 50%;
  background-color: #fff;
}
.customer text {
 font-size: 28rpx;
 text-align: center;
 width: 100%;
 display: block;
}
.customer .customer-btn {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.line2 {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
