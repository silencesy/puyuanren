<!--pages/partner/partner.wxml-->
<view>
  <van-cell-group>
    <van-field
      value="{{ username }}"
      label="名字"
      type="text"
      placeholder="请输入名字"
      bind:change="changeusername"
      bind:click-icon="onClickIcon"
    />

    <van-field
      value="{{ phone }}"
      label="手机号"
      type="number"
      bind:change="changenumber"
      placeholder="请输入手机号"
    />

    <van-field
      value="{{ address }}"
      label="代理地址"
      type="textarea"
      bind:change="changeaddress"
      placeholder="请输入代理地址"
    />
  </van-cell-group>
  <view class="btn">
    <button bindtap="submitForm">提交</button>
  </view>
</view>