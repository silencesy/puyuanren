/* pages/partner/partner.wxss */
.btn {
  position: fixed;
  width: 100%;
  height: 120rpx;
  left: 0;
  bottom: 0;
  padding: 10rpx 20rpx;
  box-sizing: border-box;
}
.btn button {
  background-color: #ff6000;
  color: #fff;
  width: 100%;
}
.license {
  display: flex;
  padding: 30rpx;
  align-items: center;
  color: #333;
  font-size: 14px;
  justify-content: space-between;
  border-bottom: 1rpx solid #eee;
}